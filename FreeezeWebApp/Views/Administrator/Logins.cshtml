@model FreeezeWebApp.Models.Database.Entities.DBLogin

@{
    Layout = "~/Views/Shared/_LayoutAdministrator.cshtml";
 }

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>#</th>
                <th>Login time (UTC)</th>
                <th>Logout time (UTC)</th>
                <th>User agent</th>
                <th>IP address</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Logins)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.UTCLoginTime</td>
                    <td>@item.UTCLogoutTime</td>
                    <td>@item.UserAgent</td>
                    <td>@item.UserIP</td>
                </tr>
            }
        </tbody>
    </table>
</div>