@model IEnumerable<FreeezeWebApp.Models.Database.Entities.DBContactFormResponse>

@{
    Layout = "~/Views/Shared/_LayoutAdministrator.cshtml";
}

<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Subject</th>
                <th>Message</th>
                <th>Added on</th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.Subject</td>
                    <td>@item.Message</td>
                    <td>@item.UTCAddedOn</td>
                    <td><a href="@Url.Action("Detail", new { item.ID })">Detail</a></td>
                    <td><a href="@Url.Action("Delete", new { item.ID })">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>